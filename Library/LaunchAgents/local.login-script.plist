<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>local.login-script</string>
  <key>ProgramArguments</key>
  <array>
    <string>sh</string>
    <string>-c</string>
    <string>
if [ -e ~/.macos_login ]
then 
  sh ~/.macos_login
fi
    </string>

  </array>
  <key>RunAtLoad</key>
  <true/>
</dict>
</plist>
